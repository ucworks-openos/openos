{
  "name": "react-electron",
  "description": "react-electron",
  "author": "ucware <support@ucware.net>",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@types/xml2js": "^0.4.5",
    "asar": "^3.0.3",
    "axios": "^0.19.2",
    "bootstrap": "^4.3.1",
    "cross-env": "6.0.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "electron-is-dev": "1.1.0",
    "electron-log": "^3.0.8",
    "emoji-mart": "^3.0.0",
    "emoji-picker-react": "^3.2.4",
    "ip": "^1.1.5",
    "macaddress": "^0.5.1",
    "moment": "^2.29.1",
    "node-notifier": "^8.0.0",
    "node-sass": "^4.14.1",
    "rc-tooltip": "^4.2.3",
    "rc-tree": "^3.9.0",
    "react": "^16.10.2",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-contenteditable": "^3.3.5",
    "react-dom": "^16.10.2",
    "react-hook-form": "^6.3.0",
    "react-image": "^4.0.3",
    "react-modal": "^3.11.2",
    "react-quill": "^1.3.5",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.1.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2",
    "typescript": "^4.0.2",
    "update-electron-app": "^1.2.0",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-actions": "^2.6.1",
    "@types/styled-components": "^5.1.2",
    "@types/webpack-env": "^1.15.3",
    "concurrently": "5.3.0",
    "electron": "11.5.0",
    "electron-builder": "^22.8.0",
    "prettier": "^2.0.5",
    "react-scripts": "3.4.3",
    "wait-on": "5.2.0"
  },
  "build": {
    "productName": "ucmMessenger",
    "appId": "com.ucware.ucm",
    "asar": true,
    "protocols": {
      "name": "ucmMessenger",
      "schemes": [
        "ucmmessenger"
      ]
    },
    "copyright": "Copyright Â© year ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "site.cfg"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": [
        "default"
      ],
      "category": "public.app-category.developer-tools",
      "icon": "public/icon.icns"
    },
    "dmg": {
      "title": "ucmMessenger",
      "icon": "public/icon.icns"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "public/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "zip",
        "tar.gz"
      ],
      "icon": "public/React.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron --no-sandbox .\"",
    "build:osx": "electron-builder --mac",
    "build:linux": "npm run build && npm run build:linux32 && npm run build:linux64",
    "build:linux32": "electron-builder --linux --ia32",
    "build:linux64": "electron-builder --linux --x64",
    "build:win": "npm run build:win32 && npm run build:win64",
    "build:win32": "electron-builder --win --ia32",
    "build:win64": "electron-builder --win --x64",
    "builder-osx": "npm run build && rm -rf dist && electron-builder -c.extraMetadata.main=build/electron.js",
    "builder-linux": "npm run build && rm -rf dist && electron-builder --linux deb tar.xz -c.extraMetadata.main=build/electron.js",
    "packager-linux": "electron-packager . ucmMessenger --overwrite --asar=true --platform=linux --arch=x64 --icon=public/React.png --prune=true --out=release-builds",
    "package-win": "electron-packager . ucmMessenger --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/icon.ico --prune=true --out=release-builds --version-string.CompanyName=UCWARE --version-string.FileDescription=UCWARE --version-string.ProductName=\"ucmMessenger\"",
    "package-mac": "electron-packager . ucmMessenger --overwrite --asar=true --platform=darwin --arch=x64 --icon=public/icon.ico --prune=true --out=release-builds",
    "create-debian-installer": "electron-installer-debian --src release-builds/ucmMessenger-linux-x64 --dest dist/installers/ --arch amd64 --config debian.json",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "create-installer-mac": "electron-installer-dmg ./release-builds/ucmMessenger\\ Electron\\ app.app ucmMessenger --out=release-builds --overwrite --icon=public/icon.ico"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
